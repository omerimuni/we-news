{% extends 'base.html.twig' %}

{% block title %}Edit Category{% endblock %}


{% block body %}
<div class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h1>Edit Category</h1>
        {{ form_start(categoryForm) }}
            {{ form_row(categoryForm.title, {
                label: 'Category title',
            }) }}
            <button type="submit" class="btn btn-primary">Edit</button> <button type="submit" class="btn btn-danger">Delete</button>
        {{ form_end(categoryForm) }}
      </div>
    </div>
  </div>
</div>
{% endblock %}